# These are the Xn --> DLY inputs done with bit access MC1-MC8: Cannot access MC0
file "../../db/bi_bit.template" { pattern
{P,           R,             PORT,            OFFSET,     ZNAM,   ONAM,  ZSV,       OSV,        SCAN}
{SDP0:,    ReadRCtrl,        S0_READ_DLY,     000,        Low,    High,  MINOR,     NO_ALARM,   "I/O Intr"}
{SDP0:,    rDLY1,            S0_READ_DLY,     001,        Low,    High,  NO_ALARM,  MAJOR,      "I/O Intr"}
{SDP0:,    rDLY2,            S0_READ_DLY,     002,        Low,    High,  NO_ALARM,  MAJOR,      "I/O Intr"}
{SDP0:,    ReadV2On,         S0_READ_DLY,     003,        Low,    High,  MINOR,     NO_ALARM,   "I/O Intr"}
{SDP0:,    ReadV2Off,        S0_READ_DLY,     004,        Low,    High,  NO_ALARM,  MINOR,      "I/O Intr"}
{SDP0:,    ReadV3On,         S0_READ_DLY,     005,        Low,    High,  MINOR,     NO_ALARM,   "I/O Intr"}
{SDP0:,    ReadV3Off,        S0_READ_DLY,     006,        Low,    High,  NO_ALARM,  MINOR,      "I/O Intr"}
{SDP0:,    ReadV4On,         S0_READ_DLY,     007,        Low,    High,  MINOR,     NO_ALARM,   "I/O Intr"}
{SDP0:,    rDLY8,            S0_READ_DLY,     008,        Low,    High,  NO_ALARM,  MAJOR,      "I/O Intr"}
{SDP0:,    rDLY9,            S0_READ_DLY,     009,        Low,    High,  NO_ALARM,  MAJOR,      "I/O Intr"}
{SDP0:,    ReadV4Off,        S0_READ_DLY,     010,        Low,    High,  NO_ALARM,  MINOR,      "I/O Intr"}
{SDP0:,    rDLY11,           S0_READ_DLY,     011,        Low,    High,  NO_ALARM,  MAJOR,      "I/O Intr"}
{SDP0:,    rDLY12,           S0_READ_DLY,     012,        Low,    High,  NO_ALARM,  MAJOR,      "I/O Intr"}
{SDP0:,    ReadRPUOn,        S0_READ_DLY,     013,        Low,    High,  MINOR,     NO_ALARM,   "I/O Intr"}
{SDP0:,    ReadRPUOff,       S0_READ_DLY,     014,        Low,    High,  NO_ALARM,  MINOR,      "I/O Intr"}
{SDP0:,    ReadRPLOn,        S0_READ_DLY,     015,        Low,    High,  MINOR,     NO_ALARM,   "I/O Intr"}
{SDP0:,    ReadRPLOff,       S0_READ_DLY,     016,        Low,    High,  NO_ALARM,  MINOR,      "I/O Intr"}
{SDP0:,    ReadTurboOn,      S0_READ_DLY,     017,        Low,    High,  MINOR,     NO_ALARM,   "I/O Intr"}
{SDP0:,    rDLY18,           S0_READ_DLY,     018,        Low,    High,  NO_ALARM,  MAJOR,      "I/O Intr"}
{SDP0:,    rDLY19,           S0_READ_DLY,     019,        Low,    High,  NO_ALARM,  MAJOR,      "I/O Intr"}
{SDP0:,    ReadTurboOff,     S0_READ_DLY,     020,        Low,    High,  NO_ALARM,  MINOR,      "I/O Intr"}
}


# -------- -------- -------- WRITING BITS -------- -------- -------- #

# For Writing the Operator Inputs (Excluding any external values) to DLY
file "../../db/bo_bit.template" { pattern
{P,        R,               PORT,           OFFSET,       ZNAM,        ONAM}
{SDP0:,    WriteRCtrl,      S0_FORCE_DLY,     000,        DISABLED,    ENABLED} 
{SDP0:,    wDLY1,           S0_FORCE_DLY,     001,        Low,    High} 
{SDP0:,    wDLY2,           S0_FORCE_DLY,     002,        Low,    High} 
{SDP0:,    WriteV2On,       S0_FORCE_DLY,     003,        Low,    High} 
{SDP0:,    WriteV2Off,      S0_FORCE_DLY,     004,        Low,    High} 
{SDP0:,    WriteV3On,       S0_FORCE_DLY,     005,        Low,    High} 
{SDP0:,    WriteV3Off,      S0_FORCE_DLY,     006,        Low,    High} 
{SDP0:,    WriteV4On,       S0_FORCE_DLY,     007,        Low,    High} 
{SDP0:,    wDLY8,           S0_FORCE_DLY,     008,        Low,    High} 
{SDP0:,    wDLY9,           S0_FORCE_DLY,     009,        Low,    High}  
{SDP0:,    WriteV4Off,      S0_FORCE_DLY,     010,        Low,    High} 
{SDP0:,    wDLY11,          S0_FORCE_DLY,     011,        Low,    High} 
{SDP0:,    wDLY12,          S0_FORCE_DLY,     012,        Low,    High}  
{SDP0:,    WriteRPUOn,      S0_FORCE_DLY,     013,        Low,    High}  
{SDP0:,    WriteRPUOff,     S0_FORCE_DLY,     014,        Low,    High}  
{SDP0:,    WriteRPLOn,      S0_FORCE_DLY,     015,        Low,    High}  
{SDP0:,    WriteRPLOff,     S0_FORCE_DLY,     016,        Low,    High}  
{SDP0:,    WriteTurboOn,    S0_FORCE_DLY,     017,        Low,    High}  
{SDP0:,    wDLY18,          S0_FORCE_DLY,     018,        Low,    High}  
{SDP0:,    wDLY19,          S0_FORCE_DLY,     019,        Low,    High}  
{SDP0:,    WriteTurboOff,   S0_FORCE_DLY,     020,        Low,    High}  
}