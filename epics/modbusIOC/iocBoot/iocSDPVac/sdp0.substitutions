{SDP0:,     TurboExhaust,  "SDP0:ReadG3.VAL CPP NMS"}
}

file "../../db/calc_ion.template" { pattern
{P,         R,             INPA}
{SDP0:,     Chamber,       "SDP0:ReadG4.VAL CPP NMS"}
}
