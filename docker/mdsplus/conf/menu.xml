<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/ file:///usr/share/openbox/menu.xsd">
    <menu id="root-menu" label="Openbox 3">
        <item label="jScope">
            <action name="Execute">
                <execute>java -jar /usr/local/mdsplus/java/classes/jScope.jar</execute>
            </action>
        </item>
        <item label="jTraverser2">
            <action name="Execute">
                <execute>java -jar /usr/local/mdsplus/java/classes/jTraverser2.jar</execute>
            </action>
        </item>
        <item label="Terminal">
            <action name="Execute">
                <execute>/usr/bin/x-terminal-emulator</execute>
            </action>
        </item>
        <item label="Htop">
            <action name="Execute">
                <execute>/usr/bin/x-terminal-emulator -e htop</execute>
            </action>
        </item>
        <item label="Rox">
            <action name="Execute">
                <execute>rox</execute>
            </action>
        </item>
    </menu>
</openbox_menu>
