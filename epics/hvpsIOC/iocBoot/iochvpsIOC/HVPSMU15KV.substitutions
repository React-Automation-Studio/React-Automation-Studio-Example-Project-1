file "../../db/calc.template" { pattern 
{D,   P,    R,         CALC,                INPA}
{MU:, PS0:, byte0,     "(A >> 8) & 0xFF",     1500}
{MU:, PS0:, byte1,     "A & 0xFF",            1500}
}
